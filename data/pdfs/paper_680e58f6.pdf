                        </mrow>
                     </msub>
                     <mo>∼</mo>
                     <mi>M</mi>
                     <mi>u</mi>
                     <mi>l</mi>
                     <mi>t</mi>
                     <mi>i</mi>
                     <mi>n</mi>
                     <mi>o</mi>
                     <mi>m</mi>
                     <mi>i</mi>
                     <mi>n</mi>
                     <mi>a</mi>
                     <mi>l</mi>
                     <mfenced separators="" open="(" close=")">
                        <mrow>
                           <mo>⋅</mo>
